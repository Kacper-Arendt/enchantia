		"styled-components": "^6.0.5",
		"zod": "^3.20.6",
		"@mui/base": "5.0.0-beta.7",
		"internationalization": "workspace:*",
		"utils": "workspace:*",
		"ui": "workspace:*"
	},
	"devDependencies": {
		"@types/jest": "^29.5.3",
		"@types/node": "^20.4.2",
		"@types/react": "^18.2.17",
		"eslint": "^8.45.0",
		"eslint-config-custom": "workspace:*",
		"jest": "^29.6.1",
		"jest-presets": "workspace:*",
		"tsconfig": "workspace:*",
		"tsup": "^7.1.0",
		"typescript": "^5.1.6"
	}
