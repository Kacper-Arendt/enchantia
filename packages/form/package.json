{
	"name": "form",
	"version": "0.0.0",
	"private": true,
	"main": "./dist/index.js",
	"source": "src/index.ts",
	"types": "./dist/index.d.ts",
	"module": "./dist/index.mjs",
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts --external react",
